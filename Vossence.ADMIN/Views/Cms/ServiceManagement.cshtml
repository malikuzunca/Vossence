@model Tuple<List<SP_Services>, int>

@{
    string pathURL = Context.Request.Path.ToString().StartsWith("/") ? Context.Request.Path.ToString().Remove(0, 1) : Context.Request.Path.ToString();
    string cdnURL = ViewBag.CdnURL;
    string topTabCls = "", subTabCls = "", required = "", checkCls = "", trClass = "", trIcon = "";

    int topTabRow = 1, subTabRow = 1, iRow = 1;
    bool topTabSelected = false;
}

@{
    <link rel="stylesheet" asp-append-version="true" href="/vendors/sweetalert2/sweetalert2.css" />
}

<div class="mt-4">
    <div class="row g-4">
        <div class="col-12 col-xl-12 order-1 order-xl-0">
            <div class="mb-9">
                <div class="card shadow-none border mb-3" data-component-card="data-component-card">
                    <div class="card-header p-4 border-bottom bg-body">
                        <div class="row g-3 justify-content-between align-items-center">
                            <div class="col-md-9"><h4 class="text-body mb-0">Hizmet Yönetimi</h4></div>
                            <div class="col-md-3 text-end">
                                <a class="btn btn-sm btn-success code-btn ms-2 fw-light collapsed" data-bs-target="#itemCreateModal" data-bs-toggle="modal">Yeni</a>
                                <a class="btn btn-sm btn-info code-btn ms-2 fw-light collapsed arrangementButton" onclick="arrangementUpdate()">Sıralama Güncelleme</a>
                            </div>
                        </div>
                    </div>
                    <div class="card-body p-0">
                        <div class="p-4 code-to-copy">
                            <div id="services" data-list='{"valueNames":["serviceName"],"page":10,"pagination":true}'>
                                <div class="mb-4">
                                    <div class="d-flex flex-wrap gap-3">
                                        <div class="search-box"><form class="position-relative"><input class="form-control search-input search" type="search" placeholder="Aranacak Terim" aria-label="Aranacak Terim" /><span class="fas fa-search search-box-icon"></span></form></div>
                                    </div>
                                </div>
                                <div class="bg-body-emphasis border-top border-bottom border-translucent position-relative top-1">
                                    <div class="table-responsive scrollbar mx-n1 px-1">
                                        <table class="table fs-9 mb-0">
                                            <thead>
                                                <tr>
                                                    <th class="white-space-nowrap align-middle ps-4" scope="col">#</th>
                                                    <th class="white-space-nowrap align-middle ps-4 sort text-center" scope="col" data-sort="serviceName">Hizmet</th>
                                                    <th class="white-space-nowrap align-middle ps-4 text-left" scope="col">Durum</th>
                                                    <th class="white-space-nowrap align-middle ps-4 text-center" scope="col">Resim</th>
                                                    <th class="white-space-nowrap align-middle ps-4 text-end" scope="col">İşlemler</th>
                                                </tr>
                                            </thead>
                                            <tbody class="list" id="products-table-body">
                                                @if (Model.Item1.Count() > 0)
                                                {
                                                    foreach (var item in Model.Item1)
                                                    {
                                                        checkCls = item.IsActive ? "checked" : "";
                                                        trClass = item.TranslateControl == 0 ? "danger blink" : "success";
                                                        trIcon = item.TranslateControl == 0 ? "exclamation-triangle" : "thumbs-up";

                                                        <tr>
                                                            <td class="text-center align-middle"><input type="number" class="form-control form-control-sm w-50 arrangementCls text-center" data-id="@item.ServiceID" value="@item.Arrangement" /></td>
                                                            <td class="servicName align-middle text-center" title="@item.ServiceName">@item.ServiceName</td>
                                                            <td class="align-middle text-left">
                                                                <div class="form-check form-switch mx-4">
                                                                    <input class="form-check-input" onchange="isActiveUpdate('@item.ServiceID', 'item_@item.ServiceID')" id="item_@item.ServiceID" type="checkbox" @checkCls />
                                                                    <label class="form-check-label" for="item_@item.ServiceID"></label>
                                                                </div>
                                                            </td>
                                                            <td class="align-middle text-center"><img width="70" src="@cdnURL/Vossence/Services/@item.ImageURL" /></td>
                                                            <td class="text-end">
                                                                <a class="btn btn-primary btn-sm btn-icon d-inline p-2" title="Güncelle" onclick="itemGet(@item.ServiceID)" data-bs-toggle="modal" data-bs-target="#itemUpdateModal"><i class="fa fa-edit"></i></a>
                                                                <a class="btn btn-success btn-sm btn-icon d-inline p-2" title="Liste Görsel" onclick="ImageGet(@item.ServiceID, '@item.ServiceName', '@item.ImageURL')" data-bs-toggle="modal" data-bs-target="#itemSingleUploadModal"><i class="fa fa-image"></i></a>
                                                                <a class="btn btn-danger btn-sm btn-icon d-inline p-2" title="Sil" onclick="itemDelete(@item.ServiceID)"><i class="fa fa-trash"></i></a>
                                                            </td>
                                                        </tr>

                                                        iRow++;
                                                    }
                                                }
                                                else
                                                {
                                                    <tr class="position-static">
                                                        <td class="align-middle ps-4 text-center" colspan="6">Kayıt Bulunamadı</td>
                                                    </tr>
                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                    <div class="row align-items-center justify-content-between py-2 pe-0 fs-9">
                                        <div class="col-auto d-flex"><p class="mb-0 d-none d-sm-block me-3 fw-semibold text-body" data-list-info="data-list-info"></p></div>
                                        <div class="col-auto d-flex"><button class="page-link" data-list-pagination="prev"><span class="fas fa-chevron-left"></span></button><ul class="mb-0 pagination"></ul><button class="page-link pe-0" data-list-pagination="next"><span class="fas fa-chevron-right"></span></button></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<form action="javascript:itemCreate()" method="post" autocomplete="off">
    <div class="modal fade" id="itemCreateModal" tabindex="-1" aria-hidden="true">
        <div class="modal-dialog modal-xl">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Kayıt Oluştur</h5>
                    <button class="btn p-1" type="button" data-bs-dismiss="modal" id="itemCreateModalCloseButton" aria-label="Close"><span class="fas fa-times fs-9"></span></button>
                </div>
                <div class="modal-body">
                    <div class="card-body p-0">
                        <div class="p-4">
                            <div class="mb-3 row">
                                <div class="col-md-12">
                                    <div class="row g-3 mb-3">
                                        <div class="form-floating mb-3">
                                            <input class="form-control" id="serviceName" type="text" placeholder="Hizmet Adı Giriniz" maxlength="500" required />
                                            <label for="serviceName">Hizmet Adı</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="mt-3 mb-3 langPanel"><h4 class="border-bottom">Dil Çevirileri</h4></div>
                                <div class="langPanel">
                                    <ul class="nav nav-underline fs-9 mb-3 pb-5" id="langTab" role="tablist">
                                        @foreach (var item in injectHelper.LangList())
                                        {
                                            topTabCls = topTabRow == 1 ? "active" : "";
                                            topTabSelected = topTabRow == 1 ? true : false;

                                            <li class="nav-item px-3">
                                                <a class="nav-link @topTabCls" id="langTab_@item.LangID" data-bs-toggle="tab" href="#lang_@item.LangID" role="tab" aria-controls="lang_@item.LangID" aria-selected="@topTabSelected">
                                                    <img src="@cdnURL/Default/flags/@item.Flag" width="40" height="25" />  @item.LangName
                                                </a>
                                            </li>
                                            topTabRow++;
                                        }
                                    </ul>
                                    <div class="tab-content mt-5" id="langTabContent">
                                        @foreach (var item in injectHelper.LangList())
                                        {
                                            subTabCls = subTabRow == 1 ? "show active" : "";
                                            required = subTabRow == 1 ? "required" : "";

                                            <div class="langCreateLoop tab-pane fade @subTabCls" data-lang="@item.LangID" id="lang_@item.LangID" role="tabpanel" aria-labelledby="langTab_@item.LangID">
                                                <div class="row mb-2 langRowNonDiv langRowNonDiv_@item.LangID">
                                                    <label class="col-sm-2 col-form-label" for="caption_@item.LangID">Hizmet Adı Başlık</label>
                                                    <div class="col-sm-10"><input class="form-control langRowNonCaption_@item.LangID" onblur="urlPrint('caption_@item.LangID', @item.LangID, 1, '@pathURL')" id="caption_@item.LangID" type="text" placeholder="@item.LangName Dili İçin Çeviri Giriniz" maxlength="200" @required /></div>
                                                </div>
                                                <div class="row mb-2 nonExternalLinkGroup langRowNonDiv langRowNonDiv_@item.LangID">
                                                    <label class="col-sm-2 col-form-label" for="title_@item.LangID">Title</label>
                                                    <div class="col-sm-10"><input class="form-control langRowNonTitle_@item.LangID" id="title_@item.LangID" type="text" placeholder="@item.LangName Dili İçin Çeviri Giriniz" maxlength="200" @required /></div>
                                                </div>
                                                <div class="row mb-2 nonExternalLinkGroup langRowNonDiv langRowNonDiv_@item.LangID">
                                                    <label class="col-sm-2 col-form-label" for="description_@item.LangID">Description</label>
                                                    <div class="col-sm-10"><input class="form-control langRowNonDescription_@item.LangID" id="description_@item.LangID" type="text" placeholder="@item.LangName Dili İçin Çeviri Giriniz" maxlength="300" @required /></div>
                                                </div>
                                                <div class="row mb-2 nonExternalLinkGroup langRowNonDiv langRowNonDiv_@item.LangID">
                                                    <label class="col-sm-2 col-form-label" for="url_@item.LangID">URL</label>
                                                    <div class="col-sm-10"><input class="form-control langRowNonURL_@item.LangID" id="url_@item.LangID" type="text" placeholder="@item.LangName Dili İçin Çeviri Giriniz" maxlength="300" @required /></div>
                                                </div>
                                                <div class="row mb-2 nonExternalLinkGroup langRowNonDiv langRowNonDiv_@item.LangID">
                                                    <label class="col-sm-2 col-form-label" for="content_@item.LangID">İçerik</label>
                                                    <div class="col-sm-10"><textarea class="tinymce langRowNonEditor_@item.LangID" id="content_@item.LangID" name="content_@item.LangID" data-tinymce="{}" placeholder="@item.LangName Dili İçin Çeviri Giriniz"></textarea></div>
                                                </div>
                                            </div>
                                            subTabRow++;
                                        }
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-primary saveButton" type="submit">Kaydet</button>
                    <button class="btn btn-outline-danger" type="button" data-bs-dismiss="modal">İptal</button>
                </div>
            </div>
        </div>
    </div>
</form>

