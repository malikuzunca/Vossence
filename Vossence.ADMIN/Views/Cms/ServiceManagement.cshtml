<form action="javascript:ImageApp()" method="post" autocomplete="off">
    <div class="modal fade" id="itemSingleUploadModal" tabindex="-1" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="itemSingleUploadCaption">Görsel</h5>
                    <button class="btn p-1" type="button" data-bs-dismiss="modal" id="itemSingleUploadModalCloseButton" aria-label="Close"><span class="fas fa-times fs-9"></span></button>
                </div>
                <div class="modal-body">
                    <div class="card-body p-0">
                        <div class="p-4">
                            <input type="hidden" id="__serviceID" />
                            <div class="mb-3">
                                <input class="form-control" id="ImageURL" type="file" />
                            </div>
                            <div class="card text-white overflow-hidden" id="ImageModalDiv">
                                <img class="card-img-top" src="@cdnURL/Services/" alt="" id="ImageModalSrc" />
                                <div class="card-img-overlay d-flex align-items-end">
                                    <div>
                                        <h4 class="card-title text-white" id="ImageModalSubCaption"></h4>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-primary ImageAppButton" type="submit">Kaydet</button>
                    <button class="btn btn-outline-danger" type="button" data-bs-dismiss="modal">İptal</button>
                </div>
            </div>
        </div>
    </div>
</form>

@section Scripts {
    <script asp-append-version="true" src="/vendors/sweetalert2/sweetalert2.js"></script>
    <script asp-append-version="true" src="/vendors/tinymce/tinymce.min.js"></script>
    <script>

        function itemCreate() {
            let serviceID = 0;
            let serviceName = $("#serviceName").val();

            let loopCount = 0, list = new Array(), iRow = 1;
            let lang = "", translate = "";

            $('.langCreateLoop').each(function () {
                let langID = $(this).attr("data-lang");
                let caption = $("#caption_" + langID).val();
                let content = $("#content_" + langID).val();
                let title = $("#title_" + langID).val();
                let description = $("#description_" + langID).val();
                let url = $("#url_" + langID).val();

                if (caption != "")
                    list.push(new langTabItem(langID, caption, null, content, title, description, null, url))
                iRow++;
            });

            spinnerApp('saveButton');

            $.ajax({
                type: "POST",
                url: "/service-app",
                data: { serviceID: serviceID, serviceName: serviceName, list: list },
                datatype: "html",
                success: function (result) {
                    if (result.resultType === 1)
                        window.location.reload()
                    else {
                        spinnerRemove('saveButton', "Kaydet");
                        newToast(result.resultCaption, result.resultMessage, "error");
                    }
                },
                error: function () {
                    spinnerRemove('saveButton', "Kaydet");
                    newToast("Hata", "Bir hata oluştu lütfen sistem yöneticinize başvurun.", "error");
                }
            });
        }

        function itemGet(serviceID) {
            $.ajax({
                type: "Get",
                url: "/service-get",
                data: { serviceID: serviceID },
                contentType: "application/json; charset=utf-8",
                success: function (result) {
                    $('.langCreateLoop').each(function (index, element) {
                        let langID = $(this).attr("data-lang");

                        $("#caption_" + langID).val("");
                        $("#title_" + langID).val("");
                        $("#description_" + langID).val("");
                        $("#url_" + langID).val("");
                        tinyMCE.get('content_' + langID).setContent("");
                    });

                    $('.langUpdateLoop').each(function (index, element) {
                        let langID = $(this).attr("data-lang");

                        $("#_caption_" + langID).val("");
                        $("#_title_" + langID).val("");
                        $("#_description_" + langID).val("");
                        $("#_url_" + langID).val("");
                        tinyMCE.get('_content_' + langID).setContent("");
                    });

                    if (result.item1 != null) {
                        $("#_serviceID").val(result.item1.serviceID)
                        $("#_serviceName").val(result.item1.serviceName)
                    }

                    if (result.item2.length > 0) {
                        $.each(result.item2, function (i, item) {
                            $("#_caption_" + item.langID).val(item.serviceSubName);
                            $("#_title_" + item.langID).val(item.title);
                            $("#_description_" + item.langID).val(item.description);
                            $("#_url_" + item.langID).val(item.url);
                            tinyMCE.get('_content_' + item.langID).setContent(item.serviceContent);
                        });
                    }
                },
                error: function () { alert("Hata") }
            });
        }

        function itemUpdate() {
            let serviceID = $("#_serviceID").val();
            let serviceName = $("#_serviceName").val();

            let loopCount = 0, list = new Array(), iRow = 1;
            let lang = "", translate = "";

            $('.langUpdateLoop').each(function () {
                let langID = $(this).attr("data-lang");
                let caption = $("#_caption_" + langID).val();
                let content = $("#_content_" + langID).val();
                let title = $("#_title_" + langID).val();
                let description = $("#_description_" + langID).val();
                let url = $("#_url_" + langID).val();

                list.push(new langTabItem(langID, caption, null, content, title, description, null, url))
            });

            spinnerApp('saveButton');

            $.ajax({
                type: "POST",
                url: "/service-app",
                data: { serviceID: serviceID, serviceName: serviceName, list: list },
                datatype: "html",
                success: function (result) {
                    if (result.resultType === 1)
                        window.location.reload()
                    else {
                        spinnerRemove('saveButton', "Kaydet");
                        newToast(result.resultCaption, result.resultMessage, "error");
                    }
                },
                error: function () {
                    spinnerRemove('saveButton', "Kaydet");
                    newToast("Hata", "Bir hata oluştu lütfen sistem yöneticinize başvurun.", "error");
                }
            });
        }

        function ImageGet(serviceID, serviceName, imageURL) {
            $("#__serviceID").val(serviceID);
            $("#itemSingleUploadCaption").html("Liste Görsel (" + serviceName + ")");

            if (imageURL != "") {
                $("#listImageModalDiv").removeClass("d-none");
                $("#listImageModalSrc").attr("src", "@cdnURL" + imageURL)
                $("#listImageModalSrc").attr("alt", serviceName)
                $("#listImageModalSubCaption").html(serviceName)
            }
            else {
                $("#listImageModalDiv").addClass("d-none");
                $("#listImageModalSrc").attr("src", "")
                $("#listImageModalSrc").attr("alt", "")
                $("#listImageModalSubCaption").html("")
            }
        }

        function ImageApp() {
            var serviceID = $("#__serviceID").val();
            var ImageURL = $('#ImageURL')[0].files;

            var model = new FormData();
            model.append('serviceID', serviceID);
            model.append('ImageURL', ImageURL[0]);

            spinnerApp("ImageAppButton")

            $.ajax({
                type: "POST",
                url: "/service-image-app",
                data: model,
                datatype: "html",
                processData: false,
                contentType: false,
                success: function (result) {
                    if (result.resultType === 1)
                        window.location.reload()
                    else {
                        spinnerRemove('ImageAppButton', "Kaydet");
                        newToast(result.resultCaption, result.resultMessage, "error");
                    }
                },
                error: function () {
                    spinnerRemove('ImageAppButton', "Kaydet");
                    newToast("Hata", "Bir hata oluştu lütfen sistem yöneticinize başvurun.", "error");
                }
            });
        }

        function isActiveUpdate(serviceID, checkID) {
            $.ajax({
                type: "POST",
                url: "/service-process",
                data: { processType: 1, serviceID: serviceID },
                datatype: "html",
                success: function (result) {
                    if (result.resultType === 1) {
                        newToast(result.resultCaption, result.resultMessage, "success");
                    }
                    else {
                        newToast(result.resultCaption, result.resultMessage, "error");
                        checkReturn(checkID);
                    }
                },
                error: function () {
                    newToast("Hata", "Bir hata oluştu lütfen sistem yöneticinize başvurun.", "error");
                    checkReturn(checkID);
                }
            });
        }

        function arrangementUpdate() {
            let arrangementBody = "";
            spinnerApp('arrangementButton');

            $('.arrangementCls').each(function () {
                arrangementBody += $(this).attr("data-id") + "," + $(this).val() + "_";
            });

            $.ajax({
                type: "POST",
                url: "/service-arrangement-update",
                data: { arrangementBody: arrangementBody },
                datatype: "html",
                success: function (result) {
                    if (result.resultType === 1)
                        window.location.reload()
                    else {
                        spinnerRemove('arrangementButton', "Sıralama Güncelle");
                        newToast(result.resultCaption, result.resultMessage, "error");
                    }
                },
                error: function () {
                    spinnerRemove('arrangementButton', "Sıralama Güncelle");
                    newToast("Hata", "Bir hata oluştu lütfen sistem yöneticinize başvurun.", "error");
                }
            });
        }

        function itemDelete(serviceID) {
            Swal.fire({
                title: 'Uyarı',
                text: "Kayıt Silinecek Emin misiniz?",
                icon: 'warning',
                showCancelButton: true,
                confirmButtonText: 'Evet',
                cancelButtonText: 'Hayır',
                customClass: {
                    confirmButton: 'btn btn-primary me-3',
                    cancelButton: 'btn btn-danger'
                },
                buttonsStyling: false
            }).then(function (result) {
                if (result.value) {
                    spinnerApp('swal2-confirm');
                    $.ajax({
                        type: "POST",
                        url: "/service-process",
                        data: { processType: 2, serviceID: serviceID },
                        datatype: "html",
                        success: function (result) {
                            spinnerRemove('swal2-confirm', 'Evet');
                            if (result.resultType === 1)
                                window.location.reload();
                            else {
                                spinnerRemove('swal2-confirm', 'Evet');
                                newToast("Hata", result);
                            }
                        },
                        error: function () {
                            spinnerRemove('swal2-confirm', 'Evet');
                            newToast("Hata", "Bir hata oluştu lütfen sistem yöneticinize başvurun.");
                        }
                    });
                } else if (result.dismiss === Swal.DismissReason.cancel) {
                    Swal.fire({
                        title: 'Uyarı',
                        text: 'Silme işlemini iptal ettiniz.',
                        icon: 'error',
                        customClass: { confirmButton: 'btn btn-primary' }
                    });
                }
            });
        }

    </script>
}